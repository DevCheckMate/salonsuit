{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug FastAPI - SalonSuite",
        "type": "debugpy",
        "request": "launch",
        "module": "uvicorn",
        "args": [
          "salonsuite.app:app",  // Substitua aqui pelo caminho correto do seu app
